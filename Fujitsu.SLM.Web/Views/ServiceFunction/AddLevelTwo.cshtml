@model AddServiceFunctionViewModel

@{
    ViewBag.Title = LevelNames.LevelTwo + " > " + (string.IsNullOrEmpty(Model.ServiceDomainName) ? "Add Service Function" : @Model.ServiceDomainName);
    var menuModel = new LevelTwoMenuModel { ServiceDomainsSelected = Model.HasServiceDomainContext, ServiceFunctionsSelected = !Model.HasServiceDomainContext };
}

@section navigation
{
    @Html.Partial("_LevelTwoMenu", menuModel)
}

@section globalcommands
{
    @Html.Action("QuickLinks", "Menu")
}

@section helpcommands
{
    @Html.ImageActionLink(Url.Content("~/Images/Help.png"), "Help", "Index", "Help", tooltip: "Help", htmlAttributes: new { id = "btnContextHelp", target = "servicedecompositionhelp" }, routeValues: new { helpKey = "addfunctionleveltwo" })
}

@section contextcommands
{

}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

<h2>@ViewBag.Title</h2>

@Html.Partial("_AddServiceFunction", Model)