@model EditServiceFunctionViewModel

@{
    ViewBag.Title = LevelNames.LevelTwo + " > " + @Model.ServiceFunction.ServiceDomainName;
}

@section navigation
{
    @Html.Partial("_LevelTwoMenu", new LevelTwoMenuModel { ServiceFunctionsSelected = true })
}

@section globalcommands
{
    @Html.Action("QuickLinks", "Menu")
}

@section helpcommands
{
    @Html.ImageActionLink(Url.Content("~/Images/Help.png"), "Help", "Index", "Help", tooltip: "Help", htmlAttributes: new { id = "btnContextHelp", target = "servicedecompositionhelp" }, routeValues: new { helpKey = "editfunctionleveltwo" })
}

@section contextcommands
{

}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

<h2>@ViewBag.Title</h2>

@Html.Partial("_EditServiceFunction", Model)