@model ViewResolverViewModel
@{
    ViewBag.Title = LevelNames.LevelOne + " > Resolvers";
}

@section navigation
{
    @Html.Partial("_LevelOneMenu", new LevelOneMenuModel { ResoversSelected = true })
}

@section globalcommands
{
    @Html.Action("QuickLinks", "Menu")
}

@section helpcommands
{
    @Html.ImageActionLink(Url.Content("~/Images/Help.png"), "Help", "Index", "Help", tooltip: "Help", htmlAttributes: new { id = "btnContextHelp", target = "servicedecompositionhelp" }, routeValues: new { helpKey = "resolverlevelone" })
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

<h2>@ViewBag.Title</h2>

@Html.Partial("_Resolvers", new ResolverGridViewModel { EditLevel = Model.EditLevel, CanMoveResolver = Model.CanMoveResolver})