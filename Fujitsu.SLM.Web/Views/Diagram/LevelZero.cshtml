@model DiagramGridViewModel

@{
    ViewBag.Title = LevelNames.LevelZero + " > Assets";
}

@section navigation
{
    @Html.Partial("_LevelZeroDiagramMenu", new DiagramMenuModel { DiagramsSelected = true })
}

@section globalcommands
{
    @Html.Action("QuickLinks", "Menu")
}

@section helpcommands
{
    @Html.ImageActionLink(Url.Content("~/Images/Help.png"), "Help", "Index", "Help", tooltip: "Help", htmlAttributes: new { id = "btnContextHelp", target = "servicedecompositionhelp" }, routeValues: new { helpKey = "diagramlevelzero" })
}

@section contextcommands
{

}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

<h2>@ViewBag.Title</h2>

@Html.Partial("_DiagramGrid", Model)