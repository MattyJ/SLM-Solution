@model AddServiceDomainViewModel

@{

    ViewBag.Title = LevelNames.LevelOne + " > " + (string.IsNullOrEmpty(Model.ServiceDeskName) ? "Add Service Domain" : @Model.ServiceDeskName);

    var menuModel = new LevelOneMenuModel { ServiceDeskInputsSelected = Model.HasServiceDeskContext, ServiceDomainsSelected = !Model.HasServiceDeskContext };
}

@section navigation
{
    @Html.Partial("_LevelOneMenu", menuModel)
}

@section globalcommands
{
    @Html.Action("QuickLinks", "Menu")
}

@section helpcommands
{
    @Html.ImageActionLink(Url.Content("~/Images/Help.png"), "Help", "Index", "Help", tooltip: "Help", htmlAttributes: new { id = "btnContextHelp", target = "servicedecompositionhelp" }, routeValues: new { helpKey = "adddomainlevelone" })
}

@section contextcommands
{

}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

<h2>@ViewBag.Title</h2>

@Html.Partial("_AddServiceDomain", Model)