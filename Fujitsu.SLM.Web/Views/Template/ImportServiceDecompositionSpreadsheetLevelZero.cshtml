@model ImportServiceDecompositionSpreadsheetViewModel

@{

    ViewBag.Title = LevelNames.LevelZero + " > " + (string.IsNullOrEmpty(Model.ServiceDeskName) ? "Import Service Decomposition Spreadsheet" : Model.ServiceDeskName);

    var menuModel = new LevelZeroMenuModel { ServiceDeskInputsSelected = Model.HasServiceDeskContext, ServiceDomainsSelected = !Model.HasServiceDeskContext };
}

@section navigation
{
    @Html.Partial("_LevelZeroMenu", menuModel)
}

@section globalcommands
{
    @Html.Action("QuickLinks", "Menu")
}

@section helpcommands
{
    @Html.ImageActionLink(Url.Content("~/Images/Help.png"), "Help", "Index", "Help", tooltip: "Help", htmlAttributes: new { id = "btnContextHelp", target = "servicedecompositionhelp" }, routeValues: new { helpKey = "importServiceDecompositionSpreadsheetLevelZero" })
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

<h2>@ViewBag.Title</h2>

@Html.Partial("_ImportServiceDecompositionSpreadsheet", Model)