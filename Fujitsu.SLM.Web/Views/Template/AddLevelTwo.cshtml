@model AddTemplateServiceDomainViewModel

@{

    ViewBag.Title = LevelNames.LevelTwo + " > " + (string.IsNullOrEmpty(Model.ServiceDeskName) ? $"Add {Model.TemplateType} Service Domain" : Model.ServiceDeskName);

    var menuModel = new LevelTwoMenuModel { ServiceDeskInputsSelected = Model.HasServiceDeskContext, ServiceDomainsSelected = !Model.HasServiceDeskContext };
}

@section navigation
{
    @Html.Partial("_LevelTwoMenu", menuModel)
}

@section globalcommands
{
    @Html.Action("QuickLinks", "Menu")
}

@section helpcommands
{
    @Html.ImageActionLink(Url.Content("~/Images/Help.png"), "Help", "Index", "Help", tooltip: "Help", htmlAttributes: new { id = "btnContextHelp", target = "servicedecompositionhelp" }, routeValues: new { helpKey = "adddomainleveltwo" })
}

@section contextcommands
{

}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

<h2>@ViewBag.Title</h2>

@Html.Partial("_AddTemplateServiceDomain", Model)