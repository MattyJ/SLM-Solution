@using Fujitsu.SLM.Web.Session
@model CustomerPackGridViewModel

@{
    var appUserContext = AppUserContext.GetAppUserContext();
    ViewBag.Title = $"Service Decomposition > Audit > {appUserContext.Current.CurrentCustomer.CustomerName}";
}

@section navigation
{
    @Html.Partial("_ServiceDecompositionMenu", new ServiceDecompositionMenuModel { AuditSelected = true })
}

@section globalcommands
{
    @Html.Action("QuickLinks", "Menu")
}

@section helpcommands
{
    @Html.ImageActionLink(Url.Content("~/Images/Help.png"), "Help", "Index", "Help", tooltip: "Help", htmlAttributes: new { id = "btnContextHelp", target = "servicedecompositionhelp" }, routeValues: new { helpKey = "decompositionaudit" })
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

<h2>@ViewBag.Title</h2>

@Html.Partial("_AuditGrid", Model)