@model ViewServiceComponentViewModel
@{
    ViewBag.Title = LevelNames.LevelOne + " > Service Components";
}

@section navigation
{
    @Html.Partial("_LevelOneMenu", new LevelOneMenuModel { ServiceComponentsSelected = true })
}

@section globalcommands
{
    @Html.Action("QuickLinks", "Menu")
}

@section helpcommands
{
    @Html.ImageActionLink(Url.Content("~/Images/Help.png"), "Help", "Index", "Help", tooltip: "Help", htmlAttributes: new { id = "btnContextHelp", target = "servicedecompositionhelp" }, routeValues: new { helpKey = "componentlevelone" })
}

@section contextcommands
{

}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

<h2>@ViewBag.Title</h2>

@Html.Partial("_ServiceComponents", new ServiceComponentGridViewModel { EditLevel = Model.EditLevel, CanMoveServiceComponent = Model.CanMoveServiceComponent})